
public class Calificacion {
	
	private double puntuacion;

	public double getPuntuacion() {
		return puntuacion;
	}

	public void setPuntuacion(double puntuacion) {
		this.puntuacion = puntuacion;
	}
	
	public char getX() {
		
		char letra;
		
		if (puntuacion >= 9) {
			letra = 'A'
		}
	}
}
